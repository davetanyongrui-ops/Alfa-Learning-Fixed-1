{
  "version": 2,
  "name": "alfa-learning-website",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_CONVEX_URL": "@convex_url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_CONVEX_URL": "@convex_url"
    }
  }
}